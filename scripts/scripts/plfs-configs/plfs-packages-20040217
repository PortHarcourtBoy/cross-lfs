# There are 2 binutils listed here.
# NATIVE_BINUTILS_VER is used only when building a native binutils
# on a non linux system when BINUTILS_VER is non FSF (ie HJL)
# This is sorted out at the end of this script.

export AUTOCONF_VER=2.59		# chapter 6
export AUTOMAKE_VER=1.8.2		# chapter 6
export BASH_VER=2.05b
export BIN86_VER=0.16.11
export BINUTILS_VER=2.14.90.0.8		# can be either FSF or HJL Binutils
export BISON_VER=1.875a
export BZIP2_VER=1.0.2
export COREUTILS_VER=5.0
export DEJAGNU_VER=1.4.4
export DIFFUTILS_VER=2.8.4
export E2FSPROGS_VER=1.34		# chapter 6
export ED_VER=0.2			# chapter 6
export EXPECT_VER=5.40.0
export FILE_VER=4.07			# chapter 6
export FINDUTILS_VER=4.2.10
export FLEX_VER=2.5.27
export GAWK_VER=3.1.3
export GCC_VER=3.3.3
export GETTEXT_VER=0.14.1
export GLIBC_VER=2.3.3-lfs-rc2
export GREP_VER=2.5.1
export GROFF_VER=1.19			# chapter 6
export GZIP_VER=1.3.5
export HOTPLUG_VER=""
export IANA_ETC_VER=""
export INETUTILS_VER=1.4.2		# chapter 6
export IPROUTE2_VER=""
export KBD_VER=1.12			# chapter 6
export KERNEL_VER=2.6.3
export LESS_VER=381			# chapter 6
export LFS_BS_VER=1.12			# chapter 6
export LIBTOOL_VER=1.5.2		# chapter 6
export LILO_VER=22.5.1
export LINUX_LIBC_HDRS_VER=""
export M4_VER=1.4
export MAKEDEV_VER=1.7
export MAKE_DEVICES_VER=1.2
export MAKE_VER=3.80
export MAN_VER=1.5m2			# chapter 6
export MODUTILS_VER=2.4.26		# chapter 6
export MODINITTOOLS_VER=3.0-pre7	# chapter 6
export NASM_VER=0.98.36
export NATIVE_BINUTILS_VER=2.14		# FSF binutils versions only
export NCURSES_VER=5.3
export NETTOOLS_VER=1.60		# chapter 6
export NFSUTILS_VER=""
export PATCH_VER=2.5.9
export PERL_VER=5.8.3
export PORTMAP_VER=""
export PROCINFO_VER=18			# chapter 6
export PROCPS_VER=3.1.15		# chapter 6
export PSMISC_VER=21.4			# chapter 6
export READLINE_VER=""
export SED_VER=4.0.9
export SHADOW_VER=4.0.4.1		# chapter 6
export STRACE_VER=""
