export VIM_VER=6.3			# chapter 6
export ZLIB_VER=1.2.2

# Sort out NATIVE_BINUTILS_VER for native builds
hostos=`uname`
if [ ! "${hostos}" = "Linux" ]; then
   # Not linux, cannot use HJL
   case ${BINUTILS_VER} in
      *.9[0-9].[0-9]* )
      ;;
      * )
         export NATIVE_BINUTILS_VER=${BINUTILS_VER}
      ;;
   esac
fi
